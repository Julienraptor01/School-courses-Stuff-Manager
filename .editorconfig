root = true

[*]
end_of_line = lf
insert_final_newline = true
indent_size = tab
indent_style = tab
tab_width = 4
trim_trailing_whitespace = true

[*.java]
ij_continuation_indent_size = 8
ij_java_imports_layout = *,|,javax.**,java.**,|,$*

[{*.yml,*.json,*.properties}]
indent_size = 2
indent_style = space
